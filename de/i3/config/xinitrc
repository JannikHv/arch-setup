# Stop background processes if necessary
pkill compton
pkill pulseaudio

# Prevent screen from going off
xset -b
xset s off
xset -dpms
xset s noblank

# Start default programs
feh --bg-scale ~/.config/i3/wallpapers/Default*
pulseaudio -D
compton -f &
synclient TapButton1=1
setxkbmap de
xdg-user-dirs-update
xdg-user-dirs-gtk-update
exec i3
